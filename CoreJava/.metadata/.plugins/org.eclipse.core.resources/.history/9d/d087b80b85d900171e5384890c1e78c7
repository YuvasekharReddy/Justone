package yuva.Practice.CoreJava;

public class JustckDynDsp {
	Inter1 in=new Hello();
     in.
}


interface Inter1{
	void g1();
	public abstract void g2();
	int A=10;
	public final int B=20;
}
class Hello implements Inter1{
	int A=16;
	int B=25;
	public void g1() {
		System.out.println("hello - > m1():"+A);
	}
	public void g2() {
		System.out.println("hello - > m2():"+B);
	}
}